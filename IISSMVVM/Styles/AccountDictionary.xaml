        <Setter Property="HorizontalAlignment" Value="Left"></Setter>
        <Setter Property="VerticalAlignment" Value="Top"></Setter>
        <Setter Property="Margin" Value="0,233,0,0"></Setter>
    </Style>
    <Style TargetType="TextBlock" x:Key="TextBlockAccountCompanyStateStyle">
        <Setter Property="Text" Value="Active/Inactive"></Setter>
        <Setter Property="Margin" Value="40,236"></Setter>
    </Style>
</ResourceDictionary>
